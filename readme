  title={Terahertz Spatio-Temporal Deep Learning Computed Tomography},
  author={Hung, Yi-Chun and Chao, Ta-Hsuan and Yu, Pojen and Yang, Shang-Hua},
  journal={arXiv preprint arXiv:2205.00324},
  year={2022}
}
